@import "../common/index.wxss";

.weui-agree__link {
  display: inline;
  color: var(--weui-LINK);
}

.topBar{
  position: absolute;
  top: 0;
  height: 6vh;
  width: 100vw; 
  height: 8vh; 
  box-sizing: border-box;
  display:flex;
}

.topBar_input{
  position: relative;
  height: 80%;
  width: 80%;
  background-color: rgb(227, 240, 255);
  margin: 1%;
  display: inline-block;
}

.topBar_button{
  position: relative;
  height: 80%;
  margin: 1%;
  background-color: rgb(202, 247, 221);
  display: flex;
  align-items: center;
  justify-content: center;
}

.map_map{
  top:8vh;
  width: 100%;
  height: 94vh;
}


